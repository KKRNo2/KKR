FrameAimbot.Name = "FrameAimbot"
FrameAimbot.Parent = FrameE
FrameAimbot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
FrameAimbot.BorderColor3 = Color3.fromRGB(0, 0, 0)
FrameAimbot.BorderSizePixel = 0
FrameAimbot.Position = UDim2.new(0.271375477, 0, 0.0648464188, 0)
FrameAimbot.Size = UDim2.new(0.728624523, 0, 0.880546093, 0)
FrameAimbot.Visible = false

AimbotButton.Name = "AimbotButton"
AimbotButton.Parent = FrameAimbot
AimbotButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AimbotButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
AimbotButton.BorderSizePixel = 0
AimbotButton.Position = UDim2.new(0.265306115, 0, 0.0620155036, 0)
AimbotButton.Size = UDim2.new(0.46938777, 0, 0.193798453, 0)
AimbotButton.Font = Enum.Font.SourceSans
AimbotButton.Text = "Aimbot"
AimbotButton.TextColor3 = Color3.fromRGB(0, 0, 0)
AimbotButton.TextScaled = true
AimbotButton.TextSize = 20.000
AimbotButton.TextWrapped = true
AimbotButton.MouseButton1Click:Connect(function()
	local Player = game.Players.LocalPlayer
	local Mouse = Player:GetMouse()
	local Camera = workspace.CurrentCamera
	local AimbotActive = false
	local FOV = 100 -- ‡∏£‡∏∞‡∏¢‡∏∞‡∏°‡∏∏‡∏°‡∏°‡∏≠‡∏á (Field of View)
	local MaxDistance = 300 -- ‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î‡∏ó‡∏µ‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÑ‡∏î‡πâ
	local FovEnabled = false -- ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î FOV

	-- ‡∏ß‡∏á‡∏Å‡∏•‡∏° FOV
	local fovCircle = Drawing.new("Circle")
	fovCircle.Radius = FOV
	fovCircle.Thickness = 2
	fovCircle.Color = Color3.new(1, 1, 1)
	fovCircle.Filled = false
	fovCircle.Visible = false -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô‡∏õ‡∏¥‡∏î‡∏ß‡∏á FOV

	-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏°‡∏µ‡∏™‡∏¥‡πà‡∏á‡∏Å‡∏µ‡∏î‡∏Ç‡∏ß‡∏≤‡∏á‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏•‡∏∞‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
	local function isVisible(target)
		local origin = Camera.CFrame.Position
		local targetPosition = target.Character.Head.Position
		local direction = (targetPosition - origin).Unit * (targetPosition - origin).Magnitude
		local raycastParams = RaycastParams.new()
		raycastParams.FilterDescendantsInstances = {Player.Character} -- ‡πÑ‡∏°‡πà‡πÉ‡∏´‡πâ Ray ‡∏ä‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏£‡∏≤‡πÄ‡∏≠‡∏á
		raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

		local raycastResult = workspace:Raycast(origin, direction, raycastParams)
		if raycastResult then
			-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ä‡∏ô‡∏Å‡∏±‡∏ö‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
			return raycastResult.Instance:IsDescendantOf(target.Character)
		end
		return true -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏≠‡∏∞‡πÑ‡∏£‡∏Å‡∏µ‡∏î‡∏Ç‡∏ß‡∏≤‡∏á
	end

	-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏´‡∏≤‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏ó‡∏µ‡πà‡∏≠‡∏¢‡∏π‡πà‡πÉ‡∏Å‡∏•‡πâ‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î‡πÉ‡∏ô‡∏ß‡∏á FOV ‡πÅ‡∏•‡∏∞‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
	local function findClosestPlayerInFOV()
		local closestPlayer = nil
		local closestDistance = FOV

		for _, target in ipairs(game.Players:GetPlayers()) do
			if target ~= Player and target.Character and target.Character:FindFirstChild("Head") then
				local headPosition = target.Character.Head.Position
				local screenPosition, onScreen = Camera:WorldToScreenPoint(headPosition)

				if onScreen then
					local mousePos = Vector2.new(Mouse.X, Mouse.Y)
					local screenPos = Vector2.new(screenPosition.X, screenPosition.Y)
					local distanceFromMouse = (mousePos - screenPos).Magnitude

					-- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏£‡∏∞‡∏¢‡∏∞‡∏ó‡∏≤‡∏á
					local playerDistance = (Camera.CFrame.Position - headPosition).Magnitude
					if distanceFromMouse < closestDistance and playerDistance <= MaxDistance and isVisible(target) then
						closestDistance = distanceFromMouse
						closestPlayer = target
					end
				end
			end
		end

		return closestPlayer
	end

	-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏´‡∏±‡∏ß (‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÉ‡∏´‡πâ‡∏Å‡∏•‡πâ‡∏≠‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏≠‡∏µ‡∏¢‡∏á‡πÇ‡∏î‡∏¢‡πÉ‡∏ä‡πâ Vector3 ‡∏Ç‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô)
	local function lockOnTarget(target)
		if target and target.Character and target.Character:FindFirstChild("Head") then
			local head = target.Character.Head
			local camPosition = Camera.CFrame.Position
			-- ‡∏Ñ‡∏≥‡∏ô‡∏ß‡∏ì‡∏ó‡∏¥‡∏®‡∏ó‡∏≤‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÇ‡∏î‡∏¢‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÄ‡∏≠‡∏µ‡∏¢‡∏á
			local direction = (head.Position - camPosition).Unit
			-- ‡∏õ‡∏£‡∏±‡∏ö CFrame ‡∏Ç‡∏≠‡∏á‡∏Å‡∏•‡πâ‡∏≠‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏•‡πá‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏´‡∏±‡∏ß‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ ‡πÇ‡∏î‡∏¢‡πÉ‡∏ä‡πâ Y axis ‡πÄ‡∏õ‡πá‡∏ô‡πÅ‡∏ô‡∏ß‡∏ï‡∏±‡πâ‡∏á‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏≠‡∏µ‡∏¢‡∏á
			Camera.CFrame = CFrame.new(camPosition, camPosition + Vector3.new(direction.X, 0, direction.Z))
		end
	end

	-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Aimbot ‡πÅ‡∏•‡∏∞ FOV ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° F
	local UIS = game:GetService("UserInputService")
	UIS.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.F then
			AimbotActive = not AimbotActive
			fovCircle.Visible = AimbotActive -- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡∏ß‡∏á FOV ‡∏ï‡∏≤‡∏°‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞ Aimbot
		end
	end)

	-- ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô FOV ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Ñ‡∏•‡∏¥‡∏Å‡∏Ç‡∏ß‡∏≤
	Mouse.Button2Down:Connect(function()
		FovEnabled = true
	end)

	-- ‡∏õ‡∏¥‡∏î FOV ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏õ‡∏•‡πà‡∏≠‡∏¢‡∏Ñ‡∏•‡∏¥‡∏Å‡∏Ç‡∏ß‡∏≤
	Mouse.Button2Up:Connect(function()
		FovEnabled = false
	end)

	-- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏Å‡∏≤‡∏£‡∏•‡πá‡∏≠‡∏Å‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÅ‡∏•‡∏∞‡∏ß‡∏á FOV ‡∏Ç‡∏ì‡∏∞ Aimbot ‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô
	game:GetService("RunService").RenderStepped:Connect(function()
		if fovCircle.Visible then
			fovCircle.Position = Vector2.new(Mouse.X, Mouse.Y)
		end

		if AimbotActive and FovEnabled then
			local target = findClosestPlayerInFOV()
			if target then
				lockOnTarget(target)
			end
		end
	end)
end)

UITextSizeConstraint_15.Parent = AimbotButton
UITextSizeConstraint_15.MaxTextSize = 20

