NoHButton.MouseButton1Click:Connect(function()
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()

	local hungerValue = character:WaitForChild("Hunger")

	hungerValue.Value = 100

	hungerValue.Changed:Connect(function(newValue)
		if newValue ~= 100 then
			hungerValue.Value = 100
		end
	end)

end)

