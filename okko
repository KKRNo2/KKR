AdminButton_2.MouseButton1Click:Connect(function()

	-- LocalScript ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô StarterPlayerScripts
	local player = game.Players.LocalPlayer
	local screenGui = player:WaitForChild("PlayerGui"):WaitForChild("AdminUi") -- ‡∏ä‡∏∑‡πà‡∏≠ ScreenGui: AdminUi
	local frame = screenGui:WaitForChild("Frame") -- ‡∏ä‡∏∑‡πà‡∏≠ Frame ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô AdminUi
	local openKey = Enum.KeyCode.Z -- ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° E ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î UI

	local function toggleUI()
		-- ‡∏™‡∏•‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ScreenGui
		screenGui.Enabled = not screenGui.Enabled

		-- ‡∏ñ‡πâ‡∏≤ ScreenGui ‡∏ñ‡∏π‡∏Å‡πÄ‡∏õ‡∏¥‡∏î‡∏≠‡∏¢‡∏π‡πà ‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡∏¥‡∏î Frame ‡∏î‡πâ‡∏ß‡∏¢
		if screenGui.Enabled then
			frame.Visible = true
		else
			frame.Visible = false
		end
	end

	-- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
	game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
		if input.KeyCode == openKey and not gameProcessed then
			toggleUI() -- ‡∏™‡∏•‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î UI ‡πÅ‡∏•‡∏∞ Frame
		end
	end)

	----------------------------

	-- LocalScript ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô StarterPlayerScripts
	local player = game.Players.LocalPlayer
	local screenGui = player:WaitForChild("PlayerGui"):WaitForChild("Mask") -- ‡∏ä‡∏∑‡πà‡∏≠ ScreenGui: AdminUi
	local frame = screenGui:WaitForChild("MainFrame") -- ‡∏ä‡∏∑‡πà‡∏≠ Frame ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô AdminUi
	local openKey = Enum.KeyCode.X -- ‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏° E ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Frame

	local function toggleFrame()
		-- ‡∏™‡∏•‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÄ‡∏â‡∏û‡∏≤‡∏∞ Frame
		frame.Visible = not frame.Visible
	end

	-- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
	game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
		if input.KeyCode == openKey and not gameProcessed then
			toggleFrame() -- ‡∏™‡∏•‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î Frame
		end
	end)

end)

