FarmButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
FarmButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
FarmButton.BorderSizePixel = 0
FarmButton.Position = UDim2.new(0.265306115, 0, 0.0620155036, 0)
FarmButton.Size = UDim2.new(0.46938777, 0, 0.193798453, 0)
FarmButton.Font = Enum.Font.SourceSans
FarmButton.Text = "FarmJob"
FarmButton.TextColor3 = Color3.fromRGB(0, 0, 0)
FarmButton.TextScaled = true
FarmButton.TextSize = 20.000
FarmButton.TextWrapped = true
FarmButton.MouseButton1Click:Connect(function()
	local farmJob = game.Workspace:WaitForChild("Farmjob")

	local function setProximityPromptHoldDurationToZero(object)
		if object:IsA("ProximityPrompt") then
			object.HoldDuration = 0
		end
		for _, child in pairs(object:GetChildren()) do
			setProximityPromptHoldDurationToZero(child)
		end
	end

	for _, model in pairs(farmJob:GetChildren()) do
		setProximityPromptHoldDurationToZero(model)
	end

end)

