	---------------------------------------------------------     ------------------------------------------------------------------------------------------
	---------------------------------------------------------------------------------------------------------------------------------------------------
	---------------------------------------------------------------------------------------------------------------------------------------------------





	local textBox = script.Parent.Frame.HitboxFrame:WaitForChild("TextBox")
	local button = script.Parent.Frame.HitboxFrame.on
	textBox.Text = "ใส่จำนวนตัวเลขขนาด"

	local isExpanding = false  -- To control the loop for hitbox expansion

	button.MouseButton1Click:Connect(function()
		local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/KKRNo2/KKR/refs/heads/main/uikkrgg')))()
		local Window = OrionLib:MakeWindow({
			Name = "KKRHUB",
			HidePremium = false,
			SaveConfig = true,
			ConfigFolder = "OrionTest"
		})

		local Tab = Window:MakeTab({
			Name = "Main",
			Icon = "rbxassetid://13440861942",
			PremiumOnly = false
		})

		local Section = Tab:AddSection({
			Name = "Script"
		})

		-- Function to convert the TextBox input to a number
		local function getHitboxSize()
			local size = tonumber(textBox.Text)
			if size then
				return Vector3.new(size, size, size)
			else
				return Vector3.new(5, 5, 5)  -- default size if input is invalid
			end
		end

		-- Function to reset hitboxes to default size
		local function resetHitbox(hitbox)
			hitbox.Size = Vector3.new(2, 2, 1)  -- Default HumanoidRootPart size
			hitbox.CanCollide = true
			hitbox.Transparency = 0
			hitbox.Color = Color3.fromRGB(255, 255, 255)  -- Reset color to white
		end

		-- Function to add a HumanoidRootPart if missing
		local function addHitboxIfMissing(character)
			if not character:FindFirstChild("HumanoidRootPart") then
				local humanoidRootPart = Instance.new("Part")
				humanoidRootPart.Name = "HumanoidRootPart"
				humanoidRootPart.Size = Vector3.new(2, 2, 1)  -- Default size
				humanoidRootPart.Anchored = false
				humanoidRootPart.CanCollide = true
				humanoidRootPart.Transparency = 0
				humanoidRootPart.Color = Color3.fromRGB(255, 255, 255)
				humanoidRootPart.Parent = character
				character.PrimaryPart = humanoidRootPart
			end
		end

		-- Function to expand all player hitboxes
		local function expandAllPlayerHitboxes()
			local localPlayer = game.Players.LocalPlayer
			local hitboxSize = getHitboxSize()

			for _, player in ipairs(game.Players:GetPlayers()) do
				pcall(function()
					if player ~= localPlayer and player.Character then
						-- Add hitbox if missing
						addHitboxIfMissing(player.Character)

						-- Expand the hitbox
						local hitbox = player.Character:FindFirstChild("HumanoidRootPart")
						if hitbox then
							hitbox.Size = hitboxSize
							hitbox.CanCollide = false
							hitbox.Transparency = 0.5
							hitbox.Color = Color3.fromRGB(255, 0, 0)
						end
					end
				end)
			end
		end

		-- Button to start expanding hitboxes continuously
		Tab:AddButton({
			Name = "Expand All Player Hitboxes",
			Callback = function()
				isExpanding = true  -- Enable the loop
				while isExpanding do
					expandAllPlayerHitboxes()
					wait(1)  -- Loop every 1 second to keep hitboxes expanded
				end
			end
		})

		-- Add a button to turn off hitbox expansion and stop the loop
		Tab:AddButton({
			Name = "Turn Off Hitbox Expansion",
			Callback = function()
				isExpanding = false  -- Stop the loop
				local function resetAllHitboxes()
					-- Reset player hitboxes
					for _, player in ipairs(game.Players:GetPlayers()) do
						pcall(function()
							if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
								resetHitbox(player.Character:FindFirstChild("HumanoidRootPart"))
							end
						end)
					end
				end

				resetAllHitboxes()  -- Call the function to reset all hitboxes
			end
		})
	end)
	--------------------------------------------------------
	local function moveFrameBack77(frame)
		-- ใช้ TweenService เพื่อทำให้การเลื่อนกลับไปตำแหน่งเดิมดูราบรื่น
		local tweenService = game:GetService("TweenService")

		-- กำหนดตำแหน่งเดิม {0.999, 0}, {0.828, 0}
		local originalPosition = UDim2.new(0.999, 0, 0.828, 0)

		-- กำหนด TweenInfo (ระยะเวลา 0.5 วินาที, การเคลื่อนไหวเป็นแบบ linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)

		-- สร้าง Tween และเล่นมัน
		local tween = tweenService:Create(frame, tweenInfo, {Position = originalPosition})
		tween:Play()
		wait(0.5)
		script.Parent.CH.Visible = false

	end

	local function moveFrameToNewPosition77(frame)
		-- ใช้ TweenService เพื่อทำให้การเลื่อนดูราบรื่น
		local tweenService = game:GetService("TweenService")

		-- กำหนดตำแหน่งใหม่ที่ต้องการให้เลื่อนไป (เปลี่ยนเป็น {0.864, 0}, {0.828, 0})
		local newPosition = UDim2.new(0.864, 0, 0.828, 0)

		-- กำหนด TweenInfo (ระยะเวลา 0.5 วินาที, การเคลื่อนไหวเป็นแบบ linear)
		local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)

		-- สร้าง Tween และเล่นมัน
		local tween = tweenService:Create(frame, tweenInfo, {Position = newPosition})
		script.Parent.CH.Visible = true
		tween:Play()

		-- ตั้งเวลา 5 วินาที แล้วเลื่อนกลับไปที่เดิม
		delay(3, function()
			moveFrameBack77(frame)
		end)
	end

	-- ฟังก์ชันเลื่อน Frame กลับไปยังตำแหน่งเริ่มต้น


	-- ตั้งค่า Position เริ่มต้นของ Frame เป็น {0.999, 0}, {0.828, 0}
	script.Parent.CH.Visible = false
	script.Parent.CH.Position = UDim2.new(0.999, 0, 0.828, 0)

	-- เมื่อกดปุ่มให้เลื่อนไปยังตำแหน่งใหม่
	button.MouseButton1Click:Connect(function()
		moveFrameToNewPosition77(script.Parent.CH)
	end)




end
coroutine.wrap(RTPOZ_fake_script)()
