-- Scripts:

local function RTPOZ_fake_script() -- KKRHUB.LocalScript 
	local script = Instance.new('LocalScript', KKRHUB)

	script.Parent.lego.Draggable = true
	script.Parent.lego.Active = true
	script.Parent.Frame.exit.l.MouseButton1Click:Connect(function()
		script.Parent.lego.Visible = true
		script.Parent.Frame.Visible = false
	end)
	script.Parent.lego.ImageButton.MouseButton1Click:Connect(function()
		script.Parent.lego.Visible = false
		script.Parent.Frame.Visible = true
	end)


	script.Parent.Frame.Draggable = true
	script.Parent.Frame.Active = true

	local button = script.Parent.Frame.BB.a -- ปุ่มที่ใช้กด
	local bu = button.Parent.a2
	local frame = script.Parent.Frame:WaitForChild("Menu") -- อ้างอิงถึง Frame ที่จะขยาย
	local tweenService = game:GetService("TweenService") -- เรียกใช้ TweenService
	local tb1 = frame.TextButton1
	local tb2 = frame.TextButton2
	local tb3 = frame.TextButton3

	---------------------------------------------------------------------------------------------------------------------------------------------------
	--[[
	Text
	]]--
	local tt1 = "🤓Cooldown 0 sec."
	local tt2 = "👑Credit"
	local tt3 = "🟥HitBox"













	---------------------------------------------------------------------------------------------------------------------------------------------------
	tb1.MouseButton1Click:Connect(function()
		script.Parent.Frame.Frame.Visible = true
		script.Parent.Frame.Frame1.Visible = false
		script.Parent.Frame.HitboxFrame.Visible = false


	end)
	tb2.MouseButton1Click:Connect(function()
		script.Parent.Frame.Frame.Visible = false
		script.Parent.Frame.Frame1.Visible = true
		script.Parent.Frame.HitboxFrame.Visible = false


	end)
	tb3.MouseButton1Click:Connect(function()
		script.Parent.Frame.Frame.Visible = false
		script.Parent.Frame.Frame1.Visible = false
		script.Parent.Frame.HitboxFrame.Visible = true


	end)

	-- ตั้งค่าขนาดเริ่มต้นของ Frame ให้เป็นศูนย์ (ปิดอยู่)
	frame.Size = UDim2.new(0, 0,0, 191) -- สูง 50% ของหน้าจอ แต่กว้าง 0 (ซ่อน)
	tb1.Size = UDim2.new(0, 0,0, 35)
	tb2.Size = UDim2.new(0, 0,0, 35)
	tb3.Size = UDim2.new(0, 0,0, 35)


	frame.Visible = false
	button.Visible = true
	bu.Visible = false

	button.MouseButton1Click:Connect(function()

		button.Visible = false
		bu.Visible = true
		frame.Visible = true
		local targetSize1 = UDim2.new(0, 76,0, 35)
		local targetSize = UDim2.new(0, 76,0, 191) -- ขยายกว้างเป็น 50% ของหน้าจอ
		local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out) -- เวลา 1 วินาที

		local tween = tweenService:Create(frame, tweenInfo, {Size = targetSize})
		local twe = tweenService:Create(tb1, tweenInfo, {Size = targetSize1})
		local twe2 = tweenService:Create(tb2, tweenInfo, {Size = targetSize1})
		local twe3 = tweenService:Create(tb3, tweenInfo, {Size = targetSize1})


		tween:Play() -- เล่นการขยายขนาด
		twe:Play()
		twe3:Play()
		twe2:Play()
		tb1.Text = tt1
		tb2.Text = tt2
		tb3.Text = tt3

	end)



	bu.MouseButton1Click:Connect(function()
		tb1.Text = ""
		tb2.Text = ""
		tb3.Text = ""



		button.Visible = true
		bu.Visible = false

		local targetSize1 = UDim2.new(0, 0,0, 35)
		local targetSize = UDim2.new(0, 0,0, 191) -- ขยายกว้างเป็น 50% ของหน้าจอ
		local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out) -- เวลา 1 วินาที

