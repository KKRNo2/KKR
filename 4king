-- Scripts:

local function CCUMUV_fake_script() -- KKRHUB4king.LocalScript 
	local script = Instance.new('LocalScript', KKRHUB4king)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local farmFolder = game.Workspace:WaitForChild("‡∏ó‡∏µ‡πà‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ú‡∏±‡∏Å") -- ‡πÇ‡∏ü‡∏•‡πÄ‡∏î‡∏≠‡∏£‡πå "‡∏ó‡∏µ‡πà‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏ú‡∏±‡∏Å"
	local farmVegetables = farmFolder:GetChildren() -- ‡∏ú‡∏±‡∏Å‡∏ü‡∏≤‡∏£‡πå‡∏°‡∏´‡∏•‡∏≤‡∏¢‡∏≠‡∏±‡∏ô
	local teleportLoopActive = false -- ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏Ç‡∏≠‡∏á‡∏•‡∏π‡∏õ
	
	local onButton = script.Parent.Frame.on -- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î
	local offButton = script.Parent.Frame.off -- ‡∏õ‡∏∏‡πà‡∏°‡∏õ‡∏¥‡∏î
	
	local function teleportAndModifyProximityPrompt()
		-- ‡∏™‡∏∏‡πà‡∏°‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏ú‡∏±‡∏Å‡∏ü‡∏≤‡∏£‡πå‡∏°
		local randomFarm = farmVegetables[math.random(1, #farmVegetables)]
	
		-- ‡∏´‡∏≤ Green Plant ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô‡∏ú‡∏±‡∏Å‡∏ü‡∏≤‡∏£‡πå‡∏°
		local greenPlant = randomFarm:FindFirstChild("Green Plant")
	
		if greenPlant then
			-- ‡∏´‡∏≤ RobPart ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô Green Plant
			local robPart = greenPlant:FindFirstChild("RobPart")
	
			if robPart then
				-- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡∏≠‡∏á RobPart
				character:SetPrimaryPartCFrame(robPart.CFrame)
	
				-- ‡∏´‡∏≤ Attachment ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô RobPart
				local attachment = robPart:FindFirstChild("Attachment")
	
				if attachment then
					-- ‡∏´‡∏≤ ProximityPrompt ‡∏†‡∏≤‡∏¢‡πÉ‡∏ô Attachment
					local proximityPrompt = attachment:FindFirstChild("ProximityPrompt")
	
					if proximityPrompt then
						-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ HoldDuration ‡πÉ‡∏´‡πâ‡πÄ‡∏ó‡πà‡∏≤‡∏Å‡∏±‡∏ö 0 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
						proximityPrompt.HoldDuration = 0
					end
				end
			end
		end
	end
	
	-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡∏π‡∏õ‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ
	local function startTeleportLoop()
		teleportLoopActive = true
		while teleportLoopActive do
			teleportAndModifyProximityPrompt()
			wait(1.5) -- ‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ (‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏õ‡∏£‡∏±‡∏ö‡πÑ‡∏î‡πâ‡∏ï‡∏≤‡∏°‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£)
		end
	end
	
	-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏´‡∏¢‡∏∏‡∏î‡∏•‡∏π‡∏õ‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ
	local function stopTeleportLoop()
		teleportLoopActive = false
	end
	
	-- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡πÇ‡∏¢‡∏á‡∏Å‡∏±‡∏ö‡∏õ‡∏∏‡πà‡∏° onButton ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏•‡∏π‡∏õ
	onButton.MouseButton1Click:Connect(function()
		startTeleportLoop()
		game.StarterGui:SetCore("SendNotification", {
			Title = "‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô";
			Text = "‚úÖ‚úÖ"; -- what the text says (ofc)
			Duration = 5;
		})	
	end)
	
	-- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡πÇ‡∏¢‡∏á‡∏Å‡∏±‡∏ö‡∏õ‡∏∏‡πà‡∏° offButton ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏´‡∏¢‡∏∏‡∏î‡∏•‡∏π‡∏õ
	offButton.MouseButton1Click:Connect(function()
		stopTeleportLoop()
		game.StarterGui:SetCore("SendNotification", {
			Title = "‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô";
			Text = "‚ùé‚ùé"; -- what the text says (ofc)
			Duration = 5;
		})
	end)
	
end
coroutine.wrap(CCUMUV_fake_script)()
