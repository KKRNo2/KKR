local function ZPMRVPK_fake_script() 
	local script = Instance.new('LocalScript', KKRHUB)

	local player = game.Players.LocalPlayer
	local inventory = player:FindFirstChild("Inventory")
	local textLabel = script.Parent.Frame.TextLabel
	local bu = textLabel.Parent.TextButton
	local box = textLabel.Parent.TextBox
	
	script.Parent.Frame.Draggable = true
	script.Parent.Frame.Active = true
	
	local scrollingFrame = textLabel.Parent.ScrollingFrame
	
	local function onButtonClicked(button)
		textLabel.Text = button.Name
	end
	
	for _, button in pairs(scrollingFrame:GetChildren()) do
		if button:IsA("TextButton") then
			button.MouseButton1Click:Connect(function()
				onButtonClicked(button)
			end)
		end
	end
	
	
	
	
	
	
	local X = script.Parent.Frame.X
	X.MouseButton1Click:Connect(function()
		script.Parent:Destroy()
	end)
	

	
bu.MouseButton1Click:Connect(function()
    if inventory then
        local valueToUpdate = inventory:FindFirstChild(box.Text) 
        if valueToUpdate then
            valueToUpdate.Value = textLabel.Text
            
            game.StarterGui:SetCore("SendNotification", {
                Title = "KKR HUB"; 
                Text = textLabel.Text" เฟี้ยวไหม";
                Icon = "rbxassetid://13867768098";
                Duration = 3;
            })
        else
            game.StarterGui:SetCore("SendNotification", {
                Title = "KKR HUB";  
                Text = "ไม่มีกุญแจนี้ในกระเป๋า"; 
                Icon = "rbxassetid://13867790056";
                Duration = 3; 
            })
        end
    end
end)
	
	
	
end
coroutine.wrap(ZPMRVPK_fake_script)()
