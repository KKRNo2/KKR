-- Scripts:

local function OBCK_fake_script() -- KKRHUB.LocalScript 
	local script = Instance.new('LocalScript', KKRHUB)

	script.Parent.Frame1.Frame.X.MouseButton1Click:Connect(function()
		script.Parent.Frame1.Visible = false
		script.Parent.FF.Visible = false
		script.Parent.Frame.Visible = false
		game.StarterGui:SetCore("SendNotification", {
			Title = "See you again";
			Text = "ðŸ˜•"; -- what the text says (ofc)
			Duration = 5;
		})
	end)
	
	script.Parent.Frame1.Frame.pop.MouseButton1Click:Connect(function()
		script.Parent.Frame1.Visible = false
		script.Parent.FF.Visible = true
	end)
	
	script.Parent.FF.ImageButton.MouseButton1Click:Connect(function()
		script.Parent.FF.Visible = false
		script.Parent.Frame1.Visible = true
	end)
	
	script.Parent.Frame1.Frame.setting.MouseButton1Click:Connect(function()
		script.Parent.Frame.Visible = true
		script.Parent.Frame1.Visible = false
	end)
	
	script.Parent.Frame.Frame.XX.MouseButton1Click:Connect(function()
		script.Parent.Frame.Visible = false
		script.Parent.Frame1.Visible = true
	end)
end
coroutine.wrap(OBCK_fake_script)()
