							script.Parent.Frame2.Frame2.Scrap.false1.Visible = true
							stopLoop() -- หยุดลูป
							break -- ออกจากลูป
						end
					end
				end
	
				wait(1) -- รอ 1 วินาทีก่อนจะลูปใหม่
			end
		end)
		coroutine.resume(loop)
	end
	
