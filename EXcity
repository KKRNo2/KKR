	--Scrap
	
	local running = false
	local loop
	
	
	local function startLoop()
		running = true
		loop = coroutine.create(function()
			while running do
				--Code
				local player = game.Players.LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
	
				local ScrapFolder = game.Workspace.Farm.Scrap
	
				local ScrapList = {}
	
				for i = 1, 11 do
					local ScrapName = "Scrap" .. i
					local Scrap = ScrapFolder:FindFirstChild(ScrapName)
					if Scrap then
						table.insert(ScrapList, Scrap)
					end
				end
	
				if #ScrapList > 0 then
					local randomScrap = ScrapList[math.random(1, #ScrapList)]
					character:SetPrimaryPartCFrame(randomScrap.CFrame)
				else
					game.StarterGui:SetCore("SendNotification", {
						Title = "ไม่พบโฟเดอร์นี้";
						Text = "🧐"; -- what the text says (ofc)
