					})	
				end
				local inventory = player:FindFirstChild("Inventory")
				if inventory then
					local bananaValue = inventory:FindFirstChild("Scrap")
					if bananaValue and bananaValue:IsA("IntValue") then
						-- ถ้า Banana ครบ 50 หยุดลูป
