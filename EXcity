-- Scripts:

local function OSGRKS_fake_script() -- KKRHUB.LocalScript 
	local script = Instance.new('LocalScript', KKRHUB)

	--เปิดปิด
	script.Parent.Frame1.X.TextButton.MouseButton1Click:Connect(function()
		script.Parent.Frame2.Visible = false
		script.Parent.Frame1.Visible = false
		script.Parent.ImageButton.Visible = false
		game.StarterGui:SetCore("SendNotification", {
			Title = "See you again";
			Text = "😕"; -- what the text says (ofc)
			Duration = 5;
		})
	end)
	
	script.Parent.Frame1.X.TextButton2.MouseButton1Click:Connect(function()
		script.Parent.Frame2.Visible = false
		script.Parent.Frame1.Visible = false
		script.Parent.ImageButton.Visible = true
	end)
	
	script.Parent.ImageButton.MouseButton1Click:Connect(function()
		script.Parent.Frame1.Visible = true
		script.Parent.ImageButton.Visible = false
	end)
	script.Parent.Frame1.Frame.TextButton.MouseButton1Click:Connect(function()
		script.Parent.Frame1.Visible = false
		script.Parent.Frame2.Visible = true
	end)
	
	script.Parent.Frame2.Frame1.ImageButton.MouseButton1Click:Connect(function()
		script.Parent.Frame1.Visible = true
		script.Parent.Frame2.Visible = false
	end)
	
	script.Parent.Frame2.Frame1.Farm.MouseButton1Click:Connect(function()
		script.Parent.Frame2.Frame2.Visible = true
		script.Parent.Frame2.Frame3.Visible = false
	end)
	script.Parent.Frame2.Frame1.TP.MouseButton1Click:Connect(function()
		script.Parent.Frame2.Frame2.Visible = false
		script.Parent.Frame2.Frame3.Visible = true
	end)
	
	script.Parent.Frame1.By.Copy.MouseButton1Click:Connect(function()
		setclipboard("ดีครัฟ")
		print("KKR HUB")
	end)
	
	--กลวย
	
	local running = false
	local loop
	local Players = game:GetService("Players")
	
	-- ฟังก์ชันเริ่มต้นลูป
	local function startLoop()
		running = true
		loop = coroutine.create(function()
			while running do
				local player = game.Players.LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
	
				-- หาตำแหน่งของ Banana ในโฟลเดอร์ Farm
				local bananaFolder = game.Workspace.Farm.Banana
	
				-- สร้างตารางสำหรับ Banana ที่จะ Teleport
				local bananaList = {}
	
				-- เพิ่ม Banana ที่มีชื่อในรูปแบบ Banana1 ถึง Banana11 ลงในตาราง
				for i = 1, 11 do
					local bananaName = "Banana" .. i
					local banana = bananaFolder:FindFirstChild(bananaName)
					if banana then
						table.insert(bananaList, banana)
					end
				end
	
				-- ตรวจสอบว่ามี Banana ในตารางหรือไม่
				if #bananaList > 0 then
					-- เลือก Banana อันใดอันหนึ่งแบบสุ่ม
					local randomBanana = bananaList[math.random(1, #bananaList)]
					character:SetPrimaryPartCFrame(randomBanana.CFrame)
				else
					game.StarterGui:SetCore("SendNotification", {
						Title = "ไม่พบโฟเดอร์นี้";
						Text = "🧐"; 
						Duration = 5;
					})
				end
	
				-- เช็คค่า Banana ใน Inventory ของผู้เล่น
				local inventory = player:FindFirstChild("Inventory")
				if inventory then
					local bananaValue = inventory:FindFirstChild("Banana")
					if bananaValue and bananaValue:IsA("IntValue") then
						-- ถ้า Banana ครบ 50 หยุดลูป
						if bananaValue.Value >= 50 then
							game.StarterGui:SetCore("SendNotification", {
								Title = "ครบ 50 กล้วยแล้ว!";
								Text = "ลูปหยุดทำงานแล้ว 🛑";
								Duration = 5;
							})
							script.Parent.Frame2.Frame2.Banana.Ture1.Visible = false
							script.Parent.Frame2.Frame2.Banana.false1.Visible = true
							stopLoop() -- หยุดลูป
							break -- ออกจากลูป
						end
					end
				end
	
				wait(1) -- รอ 1 วินาทีก่อนจะลูปใหม่
			end
		end)
		coroutine.resume(loop)
	end
	
	-- ฟังก์ชันหยุดลูป
	local function stopLoop()
		running = false
	end
	
	-- เมื่อกดปุ่ม false1 จะเริ่มต้นลูป
	script.Parent.Frame2.Frame2.Banana.false1.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2928.74805, 32.9999962, 153.417542, -0.0172345024, 1.12006555e-08, 0.999851465, 1.05002869e-08, 1, -1.10213252e-08, -0.999851465, 1.03087805e-08, -0.0172345024)
		script.Parent.Frame2.Frame2.Banana.Ture1.Visible = true
		script.Parent.Frame2.Frame2.Banana.false1.Visible = false
		startLoop() -- เริ่มลูป
	end)
	
	-- เมื่อกดปุ่ม Ture1 จะหยุดลูป
	script.Parent.Frame2.Frame2.Banana.Ture1.MouseButton1Click:Connect(function()
		if running then
			script.Parent.Frame2.Frame2.Banana.Ture1.Visible = false
			script.Parent.Frame2.Frame2.Banana.false1.Visible = true
			stopLoop() -- หยุดลูป
		end
	end)
	
	
	
	--------------------------------------------------
	
	
	--หิน
	
	local running = false
	local loop
	
	
	local function startLoop()
		running = true
		loop = coroutine.create(function()
			while running do
				--Code
				local player = game.Players.LocalPlayer
				local character = player.Character or player.CharacterAdded:Wait()
	
				local StoneFolder = game.Workspace.Farm.Stone
	
				local StoneList = {}
	
				for i = 1, 14 do
					local StoneName = "Stone" .. i
					local Stone = StoneFolder:FindFirstChild(StoneName)
					if Stone then
						table.insert(StoneList, Stone)
					end
				end
	
				if #StoneList > 0 then
					local randomStone = StoneList[math.random(1, #StoneList)]
					character:SetPrimaryPartCFrame(randomStone.CFrame)
				else
					game.StarterGui:SetCore("SendNotification", {
						Title = "ไม่พบโฟเดอร์นี้";
						Text = "🧐"; -- what the text says (ofc)
						Duration = 5;
					})	
				end
				local inventory = player:FindFirstChild("Inventory")
				if inventory then
					local bananaValue = inventory:FindFirstChild("Stone")
					if bananaValue and bananaValue:IsA("IntValue") then
						-- ถ้า Banana ครบ 50 หยุดลูป
						if bananaValue.Value >= 60 then
							game.StarterGui:SetCore("SendNotification", {
								Title = "หินครบ 60 ก้อนแล้ว!";
								Text = "ลูปหยุดทำงานแล้ว 🛑";
								Duration = 5;
							})
							script.Parent.Frame2.Frame2.Stone.Ture1.Visible = false
							script.Parent.Frame2.Frame2.Stone.false1.Visible = true
							stopLoop() -- หยุดลูป
							break -- ออกจากลูป
						end
					end
				end
	
				wait(1) -- รอ 1 วินาทีก่อนจะลูปใหม่
			end
		end)
		coroutine.resume(loop)
	end
	
	local function stopLoop()
		running = false
	end
	
	script.Parent.Frame2.Frame2.Stone.false1.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-4350.97021, 32.3387146, -191.191666, 0.562987447, 1.50780792e-08, 0.826465487, -5.68859164e-08, 1, 2.0506576e-08, -0.826465487, -5.85591877e-08, 0.562987447)
		script.Parent.Frame2.Frame2.Stone.Ture1.Visible = true
		script.Parent.Frame2.Frame2.Stone.false1.Visible = false
		startLoop()
	end)
	
	script.Parent.Frame2.Frame2.Stone.Ture1.MouseButton1Click:Connect(function()
		if running then
			script.Parent.Frame2.Frame2.Stone.Ture1.Visible = false
			script.Parent.Frame2.Frame2.Stone.false1.Visible = true
			stopLoop()
		end
	end)
	
